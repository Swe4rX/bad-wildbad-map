const Page = () => {
    const handleUpload = (file: File) => {
        // handle file upload
    };

    const onChange = (event: React.ChangeEvent<HTMLInputElement>) => {
        const files = event.target.files;
        if (files) {
            handleUpload(files[0]);
        }
    };

    return (
        <div>
            <input type="file" onChange={onChange} />
        </div>
    );
};

export default Page;